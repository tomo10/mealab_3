<h1>Signup</h1>

<%= form_for @user, :url => '/signup' do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %><br>
<%= f.label :last_name %>
<%= f.text_field :last_name %><br>
<%= f.label :username %>
<%= f.text_field :username %><br>
<%= f.label :password %>
<%= f.password_field :password %><br>
<%= f.submit %>
<% end %>
